{
    "source": {
      "include": ["./"],
      "exclude": ["./node_modules"]
    },
    "opts": {
      "destination": "./docs",
      "template": "ink-docstrap",
      "theme": "cerulean"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true,
      "default": {
        "outputSourceFiles": true
      }
    }
  }
  